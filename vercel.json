{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "css/**", "use": "@vercel/static" },
    { "src": "js/**", "use": "@vercel/static" },
    { "src": "*.xml", "use": "@vercel/static" },
    { "src": "robots.txt", "use": "@vercel/static" }
  ],
  "headers": [
    {
      "source": "/(.*\\.xml)",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" }
      ]
    }
  ]
}
